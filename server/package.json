{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "build": "npm run tsoa && swc . -d dist",
    "build:ts": "tsc --project tsconfig.json",
    "dev": "nodemon",
    "lint": "yarn eslint ./src --fix . --ext .js,.ts",
    "lint:fix": "yarn eslint ./src --fix . --ext .js,.ts --fix",
    "test:unit": "vitest --config vitest.config.ts",
    "test:e2e": "vitest --config vitest.config.e2e.ts",
    "tsoa:routes": "tsoa routes",
    "tsoa:spec": "tsoa spec",
    "tsoa": "npm run tsoa:routes && npm run tsoa:spec",
    "migrate:create": "node-pg-migrate create",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down"
  },
  "dependencies": {
    "@tsoa/runtime": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/swagger-ui-express": "^4.1.6",
    "cookie-parser": "1.4.6",
    "cors": "^2.8.5",
    "debug": "4.3.4",
    "dotenv": "16.3.2",
    "express": "4.18.2",
    "helmet": "^7.1.0",
    "http-errors": "2.0.0",
    "ioredis": "^5.3.2",
    "morgan": "1.10.0",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.104",
    "@swc/helpers": "0.5.3",
    "@swc/register": "0.1.10",
    "@types/cookie-parser": "1.4.6",
    "@types/debug": "4.1.12",
    "@types/express": "4.17.21",
    "@types/http-errors": "2.0.4",
    "@types/morgan": "1.9.9",
    "@types/node": "20",
    "@types/pg": "^8.11.0",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "@vitest/ui": "^1.2.2",
    "eslint": "8.56.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "merge": "2.1.1",
    "node-pg-migrate": "^6.2.2",
    "nodemon": "3.0.3",
    "open-cli": "8.0.0",
    "pg": "^8.11.3",
    "supertest": "^6.3.4",
    "typescript": "5.3.3",
    "unplugin-swc": "^1.4.4",
    "vitest": "^1.2.2"
  }
}
